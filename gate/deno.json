  "exports": "./badges.ts",
  "tasks": {
    "badges": "deno run --allow-all badges.ts",
    "coverage": "deno run --allow-all coverage.ts",
    "deps": "deno run --allow-all deps.ts"
  },
  "imports": {
    "@std/path": "jsr:@std/path@^1.1.4",
    "zx": "npm:zx@^8.2.4"
